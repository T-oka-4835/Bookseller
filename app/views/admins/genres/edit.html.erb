<h1>ジャンル編集</h1>

<%= form_with model: @genre, url: admins_genre_path, local: true do |f| %>
  <p>ジャンル</p>
  <%= f.text_field :name %>

  <div>
    <%= f.radio_button :is_active, "有効" %>有効
  </div>
  <div>
    <%= f.radio_button :is_active, '無効' %>無効
  </div>

  <%= f.submit "編集", class: "btn btn-primary" %>
<% end %>