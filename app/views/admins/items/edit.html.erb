<h1>商品編集ページ</h1>

<%= form_with model:@item, url: admins_item_path, local:true do |f| %>
  <h4>タイトル</h4>
  <%= f.text_field :name %>

  <h4>紹介文</h4>
  <%= f.text_area :introduction %>

  <h4>画像</h4>
  <%= f.attachment_field :image %>

  <h4>ジャンル</h4>

  <h4>販売ステータス</h4>

  <%= f.submit '本を編集' %>
<% end %>
